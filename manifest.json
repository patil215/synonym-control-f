{
  "manifest_version": 2,
  "name": "Synonym Control-F",
  "description": "Control-F that also searches through synonyms.",
  "version": "1.0",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Highlight synonyms"
  }
}